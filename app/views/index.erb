<% if current_user %>
      <div class="new-finstagram-post">
        <a href="/posts/new" class="button">New Post</a>
      </div>
<% end %>
<% @posts.each do |post| %>
<!-- locals defines which variables the partial should be able to access--> 
   <%= erb(:'shared/post', {locals: {post: post }})%>
<% end %>
